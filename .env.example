# Tesla App Environment Variables
# Copy this file to .env.local and add your actual API keys

# OpenWeather API (for weather data)
REACT_APP_API_URL=https://api.openweathermap.org/data/2.5
REACT_APP_API_KEY=your_openweather_api_key_here

# News API (for news feed)
REACT_APP_NEWS_API_KEY=your_news_api_key_here

# Google Maps API (for traffic data)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Yelp API (for local POIs)
REACT_APP_YELP_API_KEY=your_yelp_api_key_here

# Default location (Boston coordinates)
REACT_APP_DEFAULT_LAT=42.364506
REACT_APP_DEFAULT_LON=-71.038887
